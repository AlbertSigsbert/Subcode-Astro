---
export interface Props {
  title: string;
  subtitle:string;
}

const { title, subtitle } = Astro.props as Props;
---



<div class="mx-[6%] text-center py-20">
    <h1 class="font-bold text-5xl capitalize pb-3">
        {title}
    </h1>
    <p class="font-normal text-lg text-center">
      {subtitle}
    </p>
</div>